{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "arrilot/laravel-widgets": "3.13.*",
        "aws/aws-sdk-php-laravel": "^3.7",
        "barryvdh/laravel-dompdf": "^1.0",
        "blackfire/php-sdk": "^1.31",
        "doctrine/dbal": "^2.13",
        "dompdf/dompdf": "^1.0",
        "grayloon/laravel-magento-api": "*",
        "guzzlehttp/guzzle": "^7.2",
        "hulkur/laravel-hasmany-keyby": "^6.0.0",
        "laravel/forge-sdk": "^3.3",
        "laravel/framework": "^9.52",
        "laravel/passport": "^10.3",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.3",
        "lcobucci/jwt": "^4.2",
        "league/csv": "^9.6",
        "makeabledk/laravel-factory-enhanced": "^2.1|3.*|4.*",
        "milon/barcode": "^9.0",
        "sentry/sentry-laravel": "^4.0.0",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-database-mail-templates": "^3.5",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-ray": "^1.28",
        "spatie/laravel-tags": "^4.3.5",
        "thomasjohnkane/snooze": "^2.2",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-query-detector": "^1.6",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "knuckleswtf/scribe": "^4.0",
        "laravel/dusk": "*",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "*",
        "spatie/laravel-ignition": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "install-hooks": [
            "rm -rf .git/hooks",
            "ln -s ../scripts/hooks .git/hooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@install-hooks"
        ],
        "post-install-cmd": [],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
